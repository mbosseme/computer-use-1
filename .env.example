# Copy to .env (DO NOT COMMIT) and fill in values.
# This repo loads .env locally via python-dotenv for convenience.

# Required
AZURE_OPENAI_KEY=<your-azure-openai-key>

# Recommended: Responses API URL (GPT-5.2)
# Example:
# https://<your-resource>.openai.azure.com/openai/responses?api-version=2025-04-01-preview
AZURE_OPENAI_RESPONSES_URL=<your-azure-responses-api-url>

# Required if not using config/models.json
AZURE_OPENAI_DEPLOYMENT=<your-gpt-5.2-deployment-name>

# Optional legacy / fallback (some docs use this name)
AZURE_OPENAI_URL=
